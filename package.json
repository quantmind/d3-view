    },
    "keywords": [],
    "license": "Other/Proprietary",
    "description": "Work in progress :joy:",
    "repository": {
        "type": "git",
        "url": ""
    },
    "engines": {
        "node": ">= 6.0"
    },
    "directories": {
        "lib": "js/src"
    },
    "dependencies": {
        "d3-collection": "1",
        "d3-transition": "1",
        "d3-utils": "../d3-utils"
    },
    "devDependencies": {
        "aliasify": "2.0",
        "babel-eslint": "6.1",
        "babel-preset-es2015": "6.13",
        "babel-preset-es2015-rollup": "rollup/babel-preset-es2015-rollup",
        "babelify": "7.3",
        "browserify": "13.1",
        "browserify-istanbul": "2.0",
        "concat-files": "0.1",
        "eslint": "3.3",
        "eslint-plugin-disable": "0.3",
        "handlebars": "4",
        "istanbul": "0.4",
        "karma": "1.2",
        "karma-browserify": "5.1",
        "karma-coverage": "1.1",
        "karma-chrome-launcher": "2.0",
        "karma-firefox-launcher": "1.0",
        "karma-phantomjs-launcher": "1.0",
        "karma-junit-reporter": "1.1",
        "karma-es5-shim": "0.0.4",
        "karma-jasmine": "1.0",
        "karma-safari-launcher": "1.0",
        "jasmine-core": "2.4",
        "object-assign": "4.1",
        "node-sass": "3.8",
        "phantomjs-prebuilt": "2.1",
        "requirejs": "2.2",
        "rollup": "0.34",
        "rollup-plugin-json": "2.0",
        "rollup-plugin-babel": "2.6",
        "rollup-plugin-commonjs": "3.3",
        "rollup-plugin-node-resolve": "2.0",
        "rollup-plugin-replace": "1.1",
        "rollup-plugin-vue": "2.0",
        "uglify-js": "2.7",
        "watchify": "3.7"
    },
    "scripts": {
        "test": "npm run-script unit",
        "rollup": "rollup -c build/rollup.config.js",
        "source": "npm run-script lint && npm run-script rollup && npm run-script concat && npm run-script cover",
        "build": "npm run-script source && npm run-script minify",
        "cover": "karma start js/build/karma.cover.js",
        "lint": "eslint index.js js/tests js/src js/build",
        "annotate": "ng-annotate -a js/dist/fluidily.js -o fluidcloud/media/fluidcloud/fluidily.js",
        "concat": "node js/build/concat.js",
        "minify": "uglifyjs fluidcloud/media/fluidcloud/fluidily.js -c -m -o fluidcloud/media/fluidcloud/fluidily.min.js",
        "unit": "karma start build/karma.unit.js",
        "debug": "karma start js/build/karma.debug.js",
        "prepublish": "npm run test && uglifyjs dist/d3-view.js -c -m -o dist/d3-view.min.js && rm -f dist/d3-view.zip && zip -j dist/d3-view.zip -- LICENSE README.md dist/d3-view.js dist/d3-view.min.js",
        "postpublish": "VERSION=`node -e 'console.log(require(\"./package.json\").version)'`; git push && git tag -am \"Release $VERSION.\" v${VERSION} && git push --tags"
    }
}
